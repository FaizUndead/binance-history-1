    "@types/node": "^18.11.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.7.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
